#/usr/local/bin/tcsh -f

if (-f tempfile) then
	rm -f tempfile
endif

foreach f (*)
	if (-f $f) then
		set a = `echo $f | cut -c1-4`
		echo $a >>! tempfile
	endif
end

foreach f (*)
	if (-f $f) then
 	        set a = `echo $f | cut -c1-4`      
		set b = `grep $a tempfile | wc -l`
		if ($b >3) then
			if (! -d $a) mkdir $a
		mv $f $a
		endif		
	endif
end

